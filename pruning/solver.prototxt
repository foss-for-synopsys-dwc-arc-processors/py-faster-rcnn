base_lr: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 30000
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 40000
# We still use the snapshot prefix, though
snapshot_prefix: "train"
iter_size: 2
max_iter: 40000
